<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="rujukan_view_tree" model="ir.ui.view">
        <field name="name">rujukan.view.tree</field>
        <field name="model">h.rujukan</field>
        <field name="arch" type="xml">
            <tree string="" create="false">
                <field name="pasien_id" />
                <field name="rekmedis_id" />
            </tree>
        </field>
    </record>

    <record id="rujukan_view_form" model="ir.ui.view">
        <field name="name">rujukan.view.form</field>
        <field name="model">h.rujukan</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="pasien_id" />
                        <field name="rekmedis_id" />
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="rujukan_action" model="ir.actions.act_window">
        <field name="name">Action rujukan</field>
        <field name="res_model">h.rujukan</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
        <p class="o_view_nocontent_neutral_face">
                This under development
        </p>
        </field>
    </record>

    <!-- <menuitem
        id="rujukan_menu"
        name="Rujukan"
        action="rujukan_action"
        parent="h_root_menu"
        sequence="10"/> -->
</odoo>
